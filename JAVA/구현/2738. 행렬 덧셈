import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        int cnt1 = sc.nextInt();
        int cnt2 = sc.nextInt();
        int[][] aarrrr = new int[cnt1][cnt2];
        
        for(int i = 0; i < cnt1; i++){
            for(int j = 0; j < cnt2; j++){
                aarrrr[i][j] = sc.nextInt();
            }
        }
        
        for(int i = 0; i < cnt1; i++){
            for(int j = 0; j < cnt2; j++){
                aarrrr[i][j] += sc.nextInt();
            }
        }
        
        for(int i = 0; i < cnt1; i++){
            for(int j = 0; j < cnt2; j++){
                System.out.print(aarrrr[i][j] + " ");
            }
            System.out.println();
        }
    }
}
