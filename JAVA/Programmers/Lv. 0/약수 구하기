import java.util.*;


class Solution {
    public Integer[] solution(int n) {
        
        List<Integer> a = new ArrayList<Integer>();
        
        for(int i = 1; i <= n; i++){
            if(n % i == 0){
                a.add(i);
            }
        }
        
        Integer[] answer = a.toArray(new Integer[a.size()]);
        
        return answer;
    }
}
