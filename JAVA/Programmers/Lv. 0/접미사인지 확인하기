class Solution {
    public int solution(String my_string, String is_suffix) {
        int answer = 0;
        
        for(int i = 0; i<my_string.length(); i++){
            String check = my_string.substring(i);
            
            if(check.equals(is_suffix)){
                answer++;
            }
            
        }
        
        return answer;
    }
}
