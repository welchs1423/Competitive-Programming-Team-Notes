class Solution {
    public String solution(String my_string, String letter) {
        String answer = "";
        
        char a = ' ';
        
        for(int i = 0; i<my_string.length(); i++){
            a = my_string.charAt(i);
            if(!letter.equals(String.valueOf(a))){
                answer += String.valueOf(a);
            }
        }
        
        return answer;
    }
}
