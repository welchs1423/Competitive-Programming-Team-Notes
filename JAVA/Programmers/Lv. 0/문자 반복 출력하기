class Solution {
    public String solution(String my_string, int n) {
        String answer = "";
        char a = ' ';
        
        for(int i = 0; i < my_string.length(); i++){
            for(int j = 0; j < n; j++){
                a = my_string.charAt(i);
                answer += a;
            }
        }
        
        return answer;
    }
}
