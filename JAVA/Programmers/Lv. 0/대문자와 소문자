class Solution {
    public String solution(String my_string) {
        String answer = "";
        
        for(int i = 0; i<my_string.length(); i++){
            char c = my_string.charAt(i);
            String a = String.valueOf(c);
            if(c >= 'A' && c <= 'Z'){
                answer += a.toLowerCase();
            }else if(c >= 'a' && c <= 'z'){
                answer += a.toUpperCase();
            }
        }
        
        return answer;
    }
}
