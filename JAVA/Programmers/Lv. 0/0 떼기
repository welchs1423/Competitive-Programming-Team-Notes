class Solution {
    public String solution(String n_str) {
        String answer = "";
        
        int j = 0;
        
        char c = ' ';
        
        for(int i = 0; i<n_str.length(); i++){
            if('0'==n_str.charAt(i)){
                j++;
            }else{
                break;
            }
        }
        
        return n_str.substring(j);
    }
}
