import java.util.*;

class Solution {
    public int[] solution(int[] num_list) {
        List <Integer> numList = new ArrayList<Integer>();
        
        int[] answer = new int[5];
        
        for(int i = 0; i < num_list.length; i++){
            numList.add(num_list[i]);
        }
        
        Collections.sort(numList);
        
        for(int i = 0; i < 5; i++){
            answer[i] = numList.get(i);
        }
        
        return answer;
    }
}
