import java.util.*;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        int arr[] = new int[9];
        
        for(int i = 0; i < arr.length; i++){
            arr[i] = sc.nextInt();
        }
        
        int maxVal = arr[0];
        int idx = 0;
        
        for(int i = 0; i < arr.length; i++){
            if(arr[i] > maxVal){
                maxVal = arr[i];
                idx = i;
            }
        }
        
        System.out.println(maxVal);
        System.out.println(idx + 1);
        
    }
}
