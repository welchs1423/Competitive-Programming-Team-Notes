import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        int maxNum = sc.nextInt();
        int a = 0;
        int b = 0;
        
        for(int i = 0; i < maxNum; i++){
            a = sc.nextInt();
            b = sc.nextInt();
            System.out.println("Case #" + (i + 1) + ": " + a + " + " + b + " = " + (a + b));
        }
        
    }
}
