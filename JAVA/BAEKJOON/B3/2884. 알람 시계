import java.util.Scanner;

public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        
        int a = sc.nextInt();
        int b = sc.nextInt();
        
        b = b - 45;
        
        if(b < 0){
            b = 60 - Math.abs(b);
            if(a == 0){
                a = 23;
            }else{
                a = a - 1;
            }
        }else{
            b = Math.abs(b);
        }
        
        System.out.println(a + " " + b);
    }
}
