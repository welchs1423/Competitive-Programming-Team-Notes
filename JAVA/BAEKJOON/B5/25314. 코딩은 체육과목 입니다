import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        int param = sc.nextInt();
        
        String answer = "";
        
        for(int i = 0; i < param; i = i + 4){
            answer += "long ";
        }
        System.out.println(answer + "int");
    }
}
