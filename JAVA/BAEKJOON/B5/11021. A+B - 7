import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        int maxNum = sc.nextInt();
        
        for(int i = 0; i < maxNum; i++){
            System.out.println("Case #" + (i + 1) + ": " + (sc.nextInt() + sc.nextInt()));
        }
    }
}
